ORACLE_BASE=/opt/oracle
ORACLE_HOME=$ORACLE_BASE/home
NLS_LANG=American_America.WE8ISO8859P1
LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$ORACLE_HOME/lib
TNS_ADMIN=$ORACLE_HOME/network
EDI_ORACLE_OCI_SESSION_POOL_ENABLE=0

PATH=$ORACLE_HOME/bin:$PATH

export ORACLE_BASE ORACLE_HOME NLS_LANG LD_LIBRARY_PATH TNS_ADMIN EDI_ORACLE_OCI_SESSION_POOL_ENABLE
export PATH

